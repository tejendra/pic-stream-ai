{
  "name": "pic-stream-ai",
  "version": "1.0.0",
  "private": true,
  "description": "High-quality photo and video sharing app with AI features",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"yarn workspace frontend dev --host\" \"yarn workspace backend dev\"",
    "dev:frontend": "yarn workspace frontend dev",
    "dev:backend": "yarn workspace backend dev",
    "build": "./build.sh",
    "start": "yarn start:backend",
    "start:backend": "yarn workspace backend start",
    "build:frontend": "yarn workspace frontend build",
    "build:backend": "yarn workspace backend build",
    "install:all": "yarn install",
    "test": "yarn workspace frontend test && yarn workspace backend test",
    "test:frontend": "yarn workspace frontend test",
    "test:backend": "yarn workspace backend test"
  },
  "workspaces": [
    "frontend",
    "backend"
  ],
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "photo-sharing",
    "video-sharing",
    "firebase",
    "react",
    "express",
    "ai"
  ],
  "author": "",
  "license": "MIT"
} 